{
	"env": {
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"no-console": "warn",
		"prettier/prettier": "error",
		"node/no-unsuported-features/es-syntax": "off"
	}
}
